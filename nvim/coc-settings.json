{
  "coc.preferences.formatOnSaveFiletypes": [
    //"cpp",
    //"c",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "css",
    "less",
    "scss",
    "json",
    "jsonc",
    "graphql",
    "markdown",
    "vue",
    "yaml"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "prettier.printWidth": 80,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.disableSuccessMessage": true,
  "explorer.previewAction.onHover": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.width": 30,
  "explorer.file.showHiddenFiles": true,
  "cSpell.userWords": [
    "Flowbiz",
    "Itai",
    "Nemo",
    "Nimo",
    "browserslist",
    "debbug",
    "frontmatter",
    "gruvbox"
  ],
  "cSpell.enabledLanguageIds": [
    "csharp",
    "gitcommit",
    "vimwiki",
    "go",
    "vim",
    "javascript",
    "javascriptreact",
    "markdown",
    "php",
    "plaintext",
    "typescript",
    "typescriptreact",
    "yml",
    "yaml",
    "toml",
    "json"
  ],
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": [
        "dockerfile"
      ],
      "args": [
        "--stdio"
      ]
    }
  },
  "coc.source.emoji.filetypes": [
    "vimwiki",
    "markdown"
  ],
  //"coc.source.emoji.triggerCharacters": [":"],
  //"coc.source.emoji.priority": 0,
  "coc.source.word.enable": false,
  "snippets.extends": {
    "cpp": [
      "c"
    ],
    "javascriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ]
  },
  "diagnostic.virtualText": true,
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/15.0.3/clangd_15.0.3/bin/clangd",
  "snippets.ultisnips.pythonPrompt": false
}
